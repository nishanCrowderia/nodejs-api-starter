{
  "name": "nodejs-api-starter",
  "version": "0.1.0",
  "description": "Starter kit for nodejs api",
  "main": "app.js",
  "scripts": {
    "start:prod": "NODE_ENV=production babel-node --presets es2015 app.js",
    "start": "NODE_ENV=development nodemon --exec babel-node --presets es2015 app.js",
    "debug": "babel-node --presets es2015 --debug-brk app.js",
    "test": "mocha --timeout 10000"
  },
